<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="OutlineButton" parent="Widget.AppCompat.Button.Borderless">
        <item name="android:background">@drawable/outline_button_background</item>
    </style>
    <style name="FillButton" parent="Widget.AppCompat.Button.Borderless">
        <item name="android:background">@drawable/fill_button_background</item>
    </style>
    <style name="button_custom" parent="Widget.AppCompat.Button.Borderless.Colored">
        <item name="android:background">@drawable/button_custom</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textAllCaps">false</item>
    </style>
</resources>